import pila from pila

class Tubo(pila):
    
    def __init__(self, c, o):
        self.capacidad = c
        self.ocupacion = o
        self.card = 0
        
    def Cabe(self):
        return (self.card == 0)
    
    
    def Estacionar(self, v)
        if (self.card < self.capacidad):
            self.Empilar(v)
            self.card += 1
class cola:
    def __init__(self,m):
        self.cont = []
        self.tam = 0
        
    def Vacia(self):
        return (self.tam == 0)
    
    def Tope(self):
        if (self.tam > 0):
            return self.cont[0]
        print('Cola vacia')
    
    def Encolar(self, nodo):
            self.cont.append(nodo)
            self.tam += 1
   
    def Desencolar(self):
        if not self.vacia():
            self.tam -= 1
            self.cont.remove(self.cont[0])
            return
        print('Cola vacia')
    
    
  
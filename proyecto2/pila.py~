class pila:
    def __init__(self,m):
        self.cont = []
        self.tam = 0
        
    def Vacia(self):
        return (self.tam == 0)
    
    def Tope(self):
        if (self.tam > 0):
            return self.cont[-1]
        print('Pila vacia')
    
    
    def Empilar(self, nodo):
            self.cont.append(nodo)
            self.tam += 1
           
    def Desempilar(self):
        if not self.vacia():
            self.tam -= 1
            self.cont.remove(self.cont[-1])
            return
        print('La pila esta vacia')
    
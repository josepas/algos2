class pila:
    def __init__(self,m):
        self.cont = []
        self.tam = 0
        self.MAX = m
        
    def vacia(self):
        return (self.tam == 0)
    
    def tope(self):
        if (self.tam > 0):
            return self.cont[-1]
        print('Pila vacia')
    
    
    def empilar(self, nodo):
        if (self.tam < self.MAX):
            self.cont.append(nodo)
            self.tam += 1
            return
        print('La pila esta llena')
   
    def desempilar(self):
        if not self.vacia():
            self.tam -= 1
            self.cont.remove(self.cont[-1])
            return
        print('La pila esta vacia')
    